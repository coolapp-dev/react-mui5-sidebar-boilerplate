{
    "trailingSlash": "auto",
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/static/*"]
    },
    "routes": [
        {
            "route": "/images/*.{png,jpg,gif}",
            "headers": {
              "Cache-Control": "public, max-age=604800, immutable"
            }
          }
    ]
